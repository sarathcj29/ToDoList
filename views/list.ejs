<%- include("header") -%>
<div class="box" id="heading">
  <h1><%= listTitle %></h1>
</div>

<div class="box">
  <% newListItem.forEach(el => { %>
  <div class="item">
    <input type="checkbox" />
    <p><%= el %></p>
  </div>
  <%}) %>
  <form class="item" action="/" method="post">
    <input type="text" name="newItem" palceholder="New Item" autocomplete="off" />
    <button type="submit" name="listType" value="<%= listTitle %>">+</button>
  </form>
</div>
<%- include("footer") -%>
